    "from typing import List\n",
    "\n",
    "def odd_occurrences(a: List[int]):\n",
    "    d = {}\n",
    "    for v in a:\n",
    "        if (d.pop(v, 0) == 0):\n",
    "            d[v]=1\n",
    "    rslt = list(d.keys())\n",
    "  \n",
    "    return rslt[0]\n",
    "    \n",
    "a = [1, 2, 3, 2, 3, 1, 3]\n",
    "odd_occurrences(a)"
   ]
  }
